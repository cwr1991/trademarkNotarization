/**index.wxss**/
page{
  height: 100%;
}
.banner{
  position: relative;
  height: 686rpx;
}
.banner image{
  width: 100%;
}
.innerbox{
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  position: absolute;
  top:336rpx;left: 0;
}
.inner{
  width: 100%;
  height: 280rpx;
  background: #FFFFFF;
  box-shadow: 0 2px 22px 0 rgba(201,201,201,0.50);
  border-radius: 10rpx;
  padding: 55rpx 30rpx 0 30rpx;
  box-sizing: border-box;
}
.innerTop .img{
  float: left;
  margin-right: 10rpx;
  padding-top: 10rpx;
}

.innerTop .img image{
  width: 65rpx;
}

.innerTop .title{
   float: left;
   width: 420rpx;
}

.innerTop .title view{
  font-size: 32rpx;
  color: #333333;
  font-weight: 500;
}
.innerTop .title image{
  width: 177rpx;
  margin-left: 5rpx;
}
.innerTop .title text{
  font-size: 20rpx;
  color: #F44444;
}
.innerTop .but{
  width: 152rpx;
  height: 60rpx;
  float: right;
  background: #FFA944;
  border-radius: 30rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 26rpx;
  color: #FFFFFF;
  margin-top: 15rpx;
}
.innerTop .clea{
  clear: both;
}
.protocol{
  background: #fef7f7 url('https://b.86sb.com.cn/wximg/yes.png')no-repeat;  
  background-size: 27rpx 27rpx;
  background-position: 10rpx center; 
  height: 51rpx;
  margin-top: 35rpx;
  padding-left: 58rpx; 
  line-height: 51rpx;
  font-size: 24rpx;
  color: #F44444;
}
.protocol.active{
    background: #F7F7F7 url('https://b.86sb.com.cn/wximg/no.png')no-repeat;  
    background-size: 27rpx 27rpx;
    background-position: 10rpx center;
    color: #666666;
}


.service{
  padding: 0 30rpx;
}
.service .text text{
  font-size: 32rpx;
  color: #080808;
  font-weight: 500;
}
.service .text view{
  width: 170rpx;
  font-size: 26rpx;
  color: #666666;
  float: right;
  text-align: right;
}
.service .text view image{
  width: 14rpx;
  margin-left: 5rpx;
}
.service .nav .itemnav{
  width: 338rpx;
  float: left;
  position: relative
}
.service .nav{
  margin-top: 20rpx;
}
.service .nav .itemnav:last-child{
  float: right;
}
.service .nav .itemnav image{
  width: 100%;
}
.service .nav .itemnav .itemname{
  font-size: 30rpx;
  color: #FFFFFF;
  position: absolute;
  top:25rpx;left:20rpx;
}

.announcement{
  position: fixed;
  top:0;left: 0;right: 0;bottom: 0;
  margin: auto;
  background: rgba(0,0,0,0.7);
  display: none;
}
.announcement.active{
  display: block;
}
.announcement image{
  display: block;
  position: absolute;
  width: 615rpx;
  top:0;left: 0;right: 0;bottom: 0;
  margin: auto;
}

.agreementBox{
  position: fixed;
  top:0;left: 0;right: 0;bottom: 0;
  margin: auto;
}
.agreementBox .shadow{
  position: absolute;
  top:0;left: 0;right: 0;bottom: 0;
  margin: auto;
  background: rgba(0,0,0,0.7);
  z-index: 1;
}
.agreementBox .agreementInner{
  width: 650rpx;
	height: 920rpx;
	background-color: #ffffff;
	border-radius: 12rpx;
  position: absolute;
  top:0;left: 0;right: 0;bottom: 0;
  margin: auto;
  z-index: 8;
  padding-top: 50rpx;
  box-sizing: border-box;
  text-align: center;
}
.agreementBox .agreementInner .agreementClose{
  width: 40rpx;
	height: 40rpx;
  position: absolute;
  right: 30rpx;top:30rpx;
}
.agreementBox .agreementInner .agreementClose image{
  width: 100%;
}
.agreementBox .agreementInner text{
  font-size: 38rpx;
  color: #080808;
  font-weight: 600;
}
.textinfo{
  margin-top: 40rpx;
  padding: 0 68rpx;
  height: 630rpx;
  font-size: 22rpx;
  color: #666;
  text-align: left;
  overflow: auto;
  line-height: 32rpx;
}
.announcementBut{
  position: absolute;
  left: 0;right: 0;
  bottom: 0;margin: auto;
  height: 100rpx;
}
.announcementBut view{
  width: 50%;
  height: 100rpx;
  float: left;
  color: #666666;
  font-size: 30rpx;
  font-weight: 600;
  line-height: 100rpx;
}
.announcementBut view:last-child{
  background: #ff4444;
  color: #fff;
}
rich-text{
  font-size: 22rpx;
  color: #666;
  line-height: 32rpx;
}