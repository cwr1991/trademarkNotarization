page{
    height: 100%;
}
.applicant_AE{
    height: 100%;
    background: #fff;
    border-top: 40rpx solid #f5f5f5;
    box-sizing: border-box;
}
.section{
    background: #fff;
    padding:0 24rpx;
}
.list{
    height: 100rpx;
    line-height: 100rpx;
    border-bottom:1px solid #ededed;
}
.list_label{
    float: left;
    color: #080808;
    font-size: 30rpx;
}
.radio_group{
    float: left;
}
.radio{
    margin-left: 40rpx;
    color: #181818;
    font-size: 30rpx;
}
radio{
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
}
radio .wx-radio-input{

    border-radius:50%;

    width:40rpx;height:40rpx;

}
radio .wx-radio-input.wx-radio-input-checked{

    border-color:#F44444 !important;

    background:#F44444 !important;

}
radio .wx-radio-input.wx-radio-input-checked::before{

    border-radius:50%;

    width:40rpx;

    height:40rpx;

    line-height:40rpx;

    text-align:center;

    font-size:30rpx;

    color:#fff;

    background:transparent;

    transform:translate(-50%, -50%) scale(1);

    -webkit-transform:translate(-50%, -50%) scale(1);

}

